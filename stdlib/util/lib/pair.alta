export generic class Pair<T1, T2> {
  private var _first: T1? = null
  private var _second: T2? = null

  public constructor() {}

  public constructor(first: T1, second: T2) {
    this._first = first
    this._second = second
  }

  public @read function first(): ref T1 {
    return *this._first
  }

  public @read function second(): ref T2 {
    return *this._second
  }
}
