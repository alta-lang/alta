import printLine from "io"
import String from "string"
import uint64 from "types"

generator function fibonacci(): uint64 {
	let prevPrev: uint64 = 1 # n = 1
	let prev: uint64 = 1     # n = 2
	while true {
		let curr = prevPrev + prev
		yield curr
		prevPrev = prev
		prev = curr
	}
}

literal function main(): int {
	let fib = fibonacci()

	for i: int in 3...100 {
		printLine("fib(", i, ") = ", *fib.next())
	}

	return 0
}
